{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "module": "esnext",
    "target": "es2017",
    "jsx": "react-native",
    "baseUrl": "./",
    "paths": {
      "@/*": [
        "src/*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "src",
    "declarations.d.ts",
    "src/app",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ]
}
